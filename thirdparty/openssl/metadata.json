{
  "name": "camptocamp-openssl",
  "version": "1.9.0",
  "author": "Camptocamp",
  "summary": "Camptocamp OpenSSL Module",
  "license": "Apache-2.0",
  "source": "https://github.com/camptocamp/puppet-openssl",
  "project_page": "https://github.com/camptocamp/puppet-openssl",
  "issues_url": "https://github.com/camptocamp/puppet-openssl/issues",
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 2.0.0 < 5.0.0"}
